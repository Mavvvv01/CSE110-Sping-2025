import java.util.Scanner;
public class Task8{
  public static void main(String[]args){
    Scanner sc = new Scanner(System.in);
    String input = sc.nextLine();
    for(int i =0; i<input.length();i++){
      if((input.codePointAt(i)<=90 && input.codePointAt(i)>=65) || (input.codePointAt(i)<=122 && input.codePointAt(i)>=97)){
        if(i%2==0){
          if(input.codePointAt(i)<=90 && input.codePointAt(i)>=65){
            int higher_case = input.codePointAt(i);
            int lower_case = higher_case+32;
            char lower = (char)lower_case;
            System.out.print(lower);
          }else{
            System.out.print(input.charAt(i));

          }
        }else{
          if(input.codePointAt(i)<=122 && input.codePointAt(i)>=97){
            int lower_case = input.codePointAt(i);
            int higher_case = lower_case-32;
            char higher = (char)higher_case;
            System.out.print(higher);
            
          }else{
            System.out.print(input.charAt(i));

          }
        }
      }else{
        System.out.print(input.charAt(i));
      }
    }
  }
}
            